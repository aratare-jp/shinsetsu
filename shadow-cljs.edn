{:lein     {:profile "+client-dev"}
 :nrepl    {:port 9000}
 :dev-http {8000 "classpath:public"}
 :builds   {:main {:target     :browser
                   :output-dir "resources/public/js/main"
                   :asset-path "/js/main"
                   :modules    {:main {:init-fn harpocrates.client/init
                                       :entries [harpocrates.client]}}
                   :devtools   {:after-load harpocrates.client/refresh
                                :preloads   [com.fulcrologic.fulcro.inspect.preload]}}}}
